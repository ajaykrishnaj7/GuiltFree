{
  "name": "guilt-free",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "node scripts/write-version.js",
    "dev": "next dev --port 3000",
    "dev:lan": "next dev --hostname 0.0.0.0 --port 3000",
    "prebuild": "node scripts/write-version.js",
    "build": "next build",
    "start": "next start",
    "start:lan": "next start --hostname 0.0.0.0 --port 3000",
    "lint": "eslint",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:coverage:check": "jest --coverage --runInBand",
    "setup:hooks": "git config core.hooksPath .githooks",
    "test:flows": "jest src/components/AuthModal.test.tsx src/components/AuthProvider.test.tsx src/components/Navbar.test.tsx src/components/DiaryView.test.tsx src/components/EditMealModal.test.tsx --runInBand",
    "qa": "npm run lint && npm run test:flows && npm run test:coverage:check && npm run build"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@supabase/supabase-js": "^2.95.3",
    "cheerio": "^1.2.0",
    "lucide-react": "^0.565.0",
    "next": "16.1.6",
    "next-themes": "^0.4.6",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "prettier": "^3.8.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.6",
    "typescript": "^5"
  }
}
